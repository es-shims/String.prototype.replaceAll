{
	"name": "string.prototype.replaceall",
	"version": "1.0.3",
	"description": "Spec-compliant polyfill for String.prototype.replaceAll ESnext proposal",
	"main": "index.js",
	"scripts": {
		"prepublish": "safe-publish-latest",
		"pretest": "npm run lint",
		"test": "npm run tests-only",
		"posttest": "npx aud",
		"tests-only": "tape 'test/*'",
		"prelint": "evalmd *.md",
		"lint": "eslint .",
		"postlint": "es-shim-api --bound",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/es-shims/String.prototype.replaceAll.git"
	},
	"keywords": [
		"string",
		"replaceall",
		"replace",
		"regex",
		"all",
		"es2020",
		"esnext",
		"javascript",
		"spec"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/es-shims/String.prototype.replaceAll/issues"
	},
	"homepage": "https://github.com/es-shims/String.prototype.replaceAll#readme",
	"devDependencies": {
		"@es-shims/api": "^2.1.2",
		"@ljharb/eslint-config": "^16.0.0",
		"auto-changelog": "^1.16.2",
		"es5-shim": "^4.5.13",
		"es6-shim": "^0.35.5",
		"eslint": "^6.8.0",
		"evalmd": "^0.0.19",
		"functions-have-names": "^1.2.1",
		"safe-publish-latest": "^1.1.4",
		"tape": "^5.0.0-next.4"
	},
	"dependencies": {
		"define-properties": "^1.1.3",
		"es-abstract": "^1.17.4",
		"has-symbols": "^1.0.1",
		"is-regex": "^1.0.5"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false
	}
}
